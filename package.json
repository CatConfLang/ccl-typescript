{
	"name": "@tylerbu/ccl-typescript",
	"version": "0.0.0",
	"private": true,
	"description": "CCL (Colon Config Language) TypeScript packages",
	"bugs": "https://github.com/tylerbutler/ccl-typescript/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tylerbutler/ccl-typescript.git"
	},
	"license": "MIT",
	"author": "Tyler Butler <tyler@tylerbutler.com>",
	"type": "module",
	"scripts": {
		"build": "nx run-many -t build",
		"check": "biome check .",
		"check:deps": "syncpack lint",
		"ci": "nx run-many -t ci",
		"clean": "nx run-many -t clean",
		"format": "biome check . --write",
		"lint": "biome lint",
		"lint:fix": "biome lint --write",
		"syncpack:fix": "syncpack fix-mismatches && syncpack set-semver-ranges",
		"test": "nx run-many -t test",
		"test:coverage": "nx run-many -t test:coverage"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.0",
		"@changesets/cli": "^2.29.0",
		"@microsoft/api-extractor": "^7.56.0",
		"@tsconfig/node18": "^18.2.0",
		"@tsconfig/strictest": "^2.0.0",
		"@vitest/coverage-v8": "^4.0.0",
		"nx": "^22.4.0",
		"repopo": "^0.7.0",
		"rimraf": "^6.1.0",
		"syncpack": "^13.0.0",
		"typescript": "~5.9.3",
		"vitest": "^4.0.0"
	},
	"packageManager": "pnpm@10.24.0",
	"engines": {
		"node": ">=18.0.0"
	},
	"nx": {
		"includedScripts": []
	}
}
